<!DOCTYPE html>
<html>
  <head>
  <title>MoSeller</title>
  <%= csrf_meta_tag %>
  <%= render 'shared/stylesheets' %>
  <%= javascript_include_tag :all %>
  </head>
  <body>
	<header>
        <nav>
          <%= render "shared/navigation" %>
        </nav>
	</header>
    <div class="container">
      <section class="round">
         <% flash.each do |key, value| %>
          <div class="flash <%= key %>"><%= value %></div>
        <% end %>
        <%= yield %>
      </section>
      <%= render 'shared/footer' %>
    </div>
  </body>
</html>